/*M55 code for SR-QN6887A-01-401|446 by yuli at 20230921 start*/
&qupv3_se0_i2c {
    #address-cells = <1>;
    #size-cells = <0>;
    status = "ok";

    qcom,i2c-touch-active = "focaltech,fts_ts","goodix,brl-d";

    goodix_ts@5d {
        compatible = "goodix,brl-d";
        reg = <0x5d>;//0x14
        status = "ok";
        /*M55 code for QN6887A-45 by yubo at 20230911 start*/
        goodix,reset-gpio-unofficial = <&tlmm 17 0x0>;  //EVB0 and EVB1
        goodix,reset-gpio = <&tlmm 14 0x0>;             //EVB2
        /*M55 code for QN6887A-45 by yubo at 20230911 end*/
        goodix,irq-gpio = <&tlmm 51 0x2800>;
        goodix,irq-flags = <2>; /* 1:trigger rising, 2:trigger falling;*/
        goodix,panel-max-x = <1080>;
        goodix,panel-max-y = <2400>;
        goodix,panel-max-w = <255>;
        goodix,avdd-name = "avdd";
        avdd-supply = <&L3C>;
        goodix,iovdd-name = "iovdd";
        iovdd-supply = <&L2C>;
        qcom,touch-environment = "pvm";
        goodix,firmware-name = "gt9916_firmware.bin";
        goodix,config-name = "gt9916_cfg_group.bin";
    };

    focaltech_ts@38{
            compatible = "focaltech,fts_ts";
            reg = <0x38>;
            interrupt-parent = <&tlmm>;
            interrupts = <51 0x2800>;
            vdd-supply = <&L3C>; //2.8~3.3v
            vcc_i2c-supply = <&L2C>; //1.8v
            focaltech,reset-gpio-unofficial = <&tlmm 17 0x0>;  //only EVB0 and EVB1
            focaltech,reset-gpio = <&tlmm 14 0x0>;
            focaltech,irq-gpio = <&tlmm 51 0x2800>;
            focaltech,max-touch-number = <10>;
            focaltech,display-coords =  <0 0 1079 2399>;
            focaltech,ic-type = <0x56720090>;
            focaltech,touch-type = "primary";

            focaltech,touch-environment = "pvm";
           /*M55 code for SR-QN6887A-01-391 by hehaoran at 20231008 start*/
            focaltech,trusted-touch-mode = "vm_mode";
            focaltech,trusted-touch-type = "primary";
            focaltech,reset-gpio-base = <0xF10E000>;
            focaltech,intr-gpio-base = <0xF133000>;
            focaltech,trusted-touch-spi-irq = <566>;//601 754
            focaltech,trusted-touch-io-bases = <0x980000 0x910000>;//990000
            focaltech,trusted-touch-io-sizes = <0x1000 0x4000>;
            focaltech,trusted-touch-vm-gpio-list = <&tlmm 0 0 &tlmm 1 0 &tlmm 14 0 &tlmm 51 0x2008>;
            /*M55 code for SR-QN6887A-01-391 by hehaoran at 20231008 end*/
        };
};
